<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="cmsc355wideo.wideo.Settings"
    tools:ignore="ExtraText">
    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:id="@+id/buttonq"
        android:text="quality"
        android:layout_below="@+id/button"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/buttons"
        android:text="storage"
        android:layout_below="@+id/buttonf"
        android:layout_alignLeft="@+id/buttonf"
        android:layout_alignStart="@+id/buttonf"
        android:layout_marginTop="15dp" />

    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/buttonf"
        android:text="flash"
        android:layout_below="@+id/buttonq"
        android:layout_alignLeft="@+id/buttonq"
        android:layout_alignStart="@+id/buttonq"
        android:layout_marginTop="24dp" />

    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/buttona"
        android:text="about us"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/buttonv"
        android:layout_alignLeft="@+id/buttonv"
        android:layout_alignStart="@+id/buttonv" />

    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/buttonv"
        android:text="voice recognition"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/buttons"
        android:layout_alignStart="@+id/buttons" />

    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/buttonHome"
        android:text="Home"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/buttona"
        android:layout_alignLeft="@+id/buttona"
        android:layout_alignStart="@+id/buttona" />


</RelativeLayout>
